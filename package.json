{
  "name": "spotify",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "eslint": "eslint --ignore-path .gitignore \"./*/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"./*/**/*.+(ts|js|tsx)\" --write",
